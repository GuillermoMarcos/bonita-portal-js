  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "start": "./node_modules/.bin/gulp",
    "unit": "./node_modules/karma/bin/karma start karma.conf.js",
    "test": "npm run clean && ./node_modules/.bin/gulp dev && npm run unit",
